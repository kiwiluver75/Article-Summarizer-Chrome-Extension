{
  "manifest_version": 2,

  "name": "Article Summarizer",
  "description": "This extension will summarize an article that is on a page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Summarize this article!"
  },
     "permissions": [
         "activeTab"
    ]
}
